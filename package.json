{
    "name": "notes",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "start": "cross-env DEBUG=notes:* node src/app.js",
        "dev": "cross-env DEBUG=notes:* PORT=3000 nodemon src/app.js",
        "server1": "cross-env DEBUG=notes:* PORT=3001 node src/app.js",
        "server2": "cross-env DEBUG=notes:* PORT=3002 node src/app.js",
        "fs-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=fs node ./src/app.js",
        "fs-server1": "cross-env NOTES_MODEL=fs PORT=3001 node ./src/app.js",
        "fs-server2": "cross-env NOTES_MODEL=fs PORT=3002 node ./src/app.js",
        "level-start": "cross-env DEBUG=notes:* PORT=3000 NOTES_MODEL=level node ./src/app.js",
        "servers": "concurrently \"npm run server1\" \"npm run server2",
        "dl-minty": "mkdir -p src/minty && npm run dl-minty-css && npm run dl-minty-min-css",
        "dl-minty-css": "wget https://bootswatch.com/4/minty/bootstrap.css -O src/minty/bootstrap.css",
        "dl-minty-min-css": "wget https://bootswatch.com/4/minty/bootstrap.min.css -O src/minty/bootstrap.min.css",
        "sqlite3-setup": "sqlite3 notes.sqlite3 --init src/models/sql/schema-sqlite3.sql",
        "sqlite3-start": "cross-env DEBUG=notes:* NOTES_MODEL=sqlite3 node ./src/app.js",
        "sqlite3-server1": "cross-env NOTES_MODEL=sqlite3 PORT=3001 node ./src/app.js",
        "sqlite3-server2": "cross-env NOTES_MODEL=sqlite3 PORT=3002 node ./src/app.js",
        "sequelize-start": "cross-env DEBUG=notes:* NOTES_MODEL=sequelize node ./src/app.js",
        "sequelize-server1": "cross-env NOTES_MODEL=sequelize PORT=3001 node ./src/app.js",
        "sequelize-server2": "cross-env NOTES_MODEL=sequelize PORT=3002 node ./src/app.js",
        "mongodb-start": "cross-env DEBUG=notes:* node ./src/app.js",
        "mongodb-server1": "cross-env DEBUG=notes:* PORT=3001 node ./src/app.js",
        "mongodb-server2": "cross-env DEBUG=notes:* PORT=3002 node ./src/app.js"
    },
    "dependencies": {
        "bcrypt": "^5.1.1",
        "bootstrap": "4.5.x",
        "concurrently": "^9.1.0",
        "cookie-parser": "~1.4.4",
        "cross-env": "^7.0.3",
        "debug": "~2.6.9",
        "dotenv": "^16.4.7",
        "express": "~4.16.1",
        "express-session": "1.17.x",
        "feather-icons": "4.25.x",
        "fs-extra": "^11.2.0",
        "hbs": "~4.0.4",
        "http-errors": "~1.6.3",
        "jquery": "3.5.x",
        "js-yaml": "^4.1.0",
        "level": "^9.0.0",
        "mongodb": "^6.12.0",
        "morgan": "~1.9.1",
        "passport": "^0.4.1",
        "passport-local": "^1.0.0",
        "passport-twitter": "^1.0.4",
        "popper.js": "1.16.x",
        "rotating-file-stream": "^3.2.5",
        "sequelize": "^6.37.5",
        "session-file-store": "1.4.x",
        "sqlite3": "^5.1.7",
        "superagent": "^10.1.1"
    },
    "devDependencies": {
        "capture-console": "^1.0.2",
        "nodemon": "^3.1.7"
    }
}
